{
  "name": "@teawithsand/mintay-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format:fix": "prettier ./src --write",
    "format:check": "prettier ./src --check",
    "lint:fix": "eslint ./src --fix",
    "test:mutate": "stryker run",
    "test": "vitest",
    "test:ss": "vitest -u",
    "build:check": "tsc --noEmit",
    "fix": "npm run format:fix && npm run lint:fix",
    "pre-commit-check": "npm run build:check && npm run lint && npm run format:check",
    "pre-push-check": "npm run build:check && npm run lint && npm run format:check && npm run test",
    "prepublishOnly": "npm run build && npm run lint && npm run format:check && npm run test",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint ./src --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@teawithsand/fstate": "workspace:*",
    "@teawithsand/booklibr": "workspace:*",
    "@mantine/core": "~8.0.2",
    "@mantine/hooks": "~8.0.2",
    "postcss": "~8.5.4",
    "postcss-preset-mantine": "~1.17.0",
    "postcss-simple-vars": "~7.0.1",
    "sass-embedded": "~1.89.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.26.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/coverage-istanbul": "^3.1.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "vitest": "~3.1.3",
    "@stryker-mutator/vitest-runner": "~9.0.1",
    "sass-embedded": "~1.89.1"
  }
}
